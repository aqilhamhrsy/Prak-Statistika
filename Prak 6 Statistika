summary(iris)
head(iris)
by(iris$Petal.Length, iris$Species, summary)
mean(iris$Sepal.Length)
var(iris$Sepal.Length)
boxplot (iris$Petal.Length)
boxplot(Petal.Length ~ Species, data=iris, main="Petal Length", xlab="Species", ylab="Length")

hist(iris$Petal.Length)
hist(iris$Sepal.Width)
hist(iris$Sepal.Width[iris$Species=="setosa"])
hist(iris$Sepal.Width[iris$Species=="versicolor"])
d=density(iris$Petal.Length)
plot(d)
d=density(iris$Sepal.Width[iris$Species=="versicolor"])
plot (d)

aqilha = read.csv("D:/Kuliah Aqilha/Semester 3/Statistika/DirectMarketing.csv")
hist(aqilha$AmountSpent)
hist(aqilha$AmountSpent[aqilha$Married=="Married"])
hist(aqilha$AmountSpent[aqilha$Married=="Single"])
d=density(aqilha$AmountSpent)
plot(d)
d=density(aqilha$AmountSpent[aqilha$Married=="Married"])
plot (d)

